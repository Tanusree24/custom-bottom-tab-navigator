{
  "expo": {
    "name": "Innovative App",
    "slug": "Innovative-app",
    "description": "4 Way Technology is all about innoavtion.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/innovation-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/innovation-splash-screen.png",
